{
  "name": "@payfunc/model-base",
  "version": "0.1.30",
  "description": "Base Data models for Intergiro PSP and Acquirer.",
  "author": "Intergiro",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "browser": {
    "./dist/fetch": "./dist/fetch.browser"
  },
  "git": {
    "tagName": "v${version}"
  },
  "github": {
    "release": true
  },
  "jest": {
    "transform": {
      "^.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(cryptly|authly|@payfunc|gracely|isoly|servly|cloud-http)/.*)"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.test.json"
      }
    },
    "testEnvironment": "node",
    "testRegex": "((\\.|/)(test|spec))(\\.|\\/.+)(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "node_modules/",
      "dist/"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "preset": "ts-jest",
    "testMatch": null
  },
  "scripts": {
    "dev": "watch 'npm run build' .",
    "build": "tsc -p .",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "fix": "eslint '**/*.{js,ts,tsx}' --fix",
    "test": "jest --maxWorkers=2",
    "test:watch": "watch jest",
    "prepare": "npm run build",
    "clean": "rm -rf dist node_modules coverage"
  },
  "dependencies": {
    "authly": "0.2.4",
    "gracely": "0.1.8",
    "isoly": "0.1.19",
    "isomorphic-fetch": "^3.0.0",
    "selectively": "^0.0.70"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^27.2.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standardize": "^0.9.1",
    "eslint-plugin-prettierx": "github:robinnedergard/eslint-plugin-prettierx#master",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27",
    "prettierx": "github:robinnedergard/prettierx#newline-before-clause-sync",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3",
    "watch": "^1.0.2"
  }
}
